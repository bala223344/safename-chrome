{
  "name": "Safename Pro",
  "version": "1.0.9",
  "description": "Auto-detect selected SafeName and Address with tooltip helper.",
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*",
    "background"
  ],
  "background": {
    "scripts": [
      "background.js",
      "jquery.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/x16.png",
    "32": "images/x32.png",
    "48": "images/x48.png",
    "128": "images/x128.png"
  },
  "manifest_version": 2
}